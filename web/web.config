  <appSettings/>
  <connectionStrings>

    <add name="OracleConnectionString" connectionString="Data Source= (DESCRIPTION=(ADDRESS_LIST = (ADDRESS = (PROTOCOL = TCP)(HOST = vmwaters1.rtpnc.epa.gov)(PORT = 1521)))(CONNECT_DATA = (SID = owpub) (SERVER = DEDICATED)));User Id=nlfwa_map;Password=****;"/>

  </connectionStrings>
  <system.net>
    <mailSettings>
      <smtp>
        <!-- Development/Staging -->
        <network host="unixmail.rtpnc.epa.gov" />
      </smtp>
    </mailSettings>
  </system.net>
  <system.web>
    <sessionState timeout="1440" />
    <healthMonitoring>
      <eventMappings>
        <add name="All Errors" type="System.Web.Management.WebBaseErrorEvent,System.Web,Version=2.0.0.0,Culture=neutral,PublicKeyToken=b03f5f7f11d50a3a" startEventCode="0" endEventCode="2147483647" />
      </eventMappings>
      <providers>
        <clear />

        <!-- Production -->
        <add type="System.Web.Management.SimpleMailWebEventProvider" name="EmailWebEventProvider" buffer="false" from="nlfa-support@rti.org" to="nlfa-support@rti.org" subjectPrefix="NLFA Web Site Error: " />

      </providers>
      <rules>
        <clear />

        <add name="All Errors Default" eventName="All Errors" provider="EmailWebEventProvider" profile="Default" minInstances="1" maxLimit="Infinite" minInterval="00:00:00" />

      </rules>
    </healthMonitoring>
    <httpRuntime maxRequestLength="102400" executionTimeout="600" requestLengthDiskThreshold="1024" />
